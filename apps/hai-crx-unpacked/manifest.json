{
  "manifest_version": 3,
  "name": "haix",
  "version": "0.0.1",
  "description": "HAI Chrome Extension",
  "action": {
    "default_title": "Hai"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": [
    "tabs"
  ],
  "host_permissions": [],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}